{
  "name": "@justdx/components",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./globals.css": "./src/styles/globals.css",
    "./shadcn-primitives": {
      "import": "./dist/shadcn-primitives/index.js",
      "types": "./dist/shadcn-primitives/index.d.ts"
    },
    "./atoms/*": {
      "import": "./src/atoms/*/index.ts",
      "types": "./dist/atoms/*/index.d.ts"
    },
    "./molecules/*": {
      "import": "./src/molecules/*/index.ts",
      "types": "./dist/molecules/*/index.d.ts"
    },
    "./organisms/*": {
      "import": "./src/organisms/*/index.ts",
      "types": "./dist/organisms/*/index.d.ts"
    },
    "./utils/*": {
      "import": "./src/utils/*.ts",
      "types": "./dist/utils/*.d.ts"
    },
    "./hooks/*": {
      "import": "./src/hooks/*",
      "types": "./dist/hooks/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist .turbo storybook-static",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\""
  },
  "devDependencies": {
    "@justdx/config": "workspace:*",
    "@storybook/addon-docs": "10.2.0",
    "@storybook/addon-links": "10.2.0",
    "@storybook/addon-themes": "^10.2.0",
    "@storybook/react": "10.2.0",
    "@storybook/react-vite": "10.2.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.4",
    "esbuild-plugin-tsconfig-paths": "^1.0.2",
    "eslint": "^9.17.0",
    "eslint-plugin-storybook": "10.2.0",
    "prettier": "^3.4.2",
    "storybook": "10.2.0",
    "tailwindcss": "^4.0.0",
    "tsup": "^8.3.5",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^6.0.4"
  },
  "peerDependencies": {
    "react": "^18.3.1 || ^19.0.0",
    "react-dom": "^18.3.1 || ^19.0.0"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.562.0",
    "next-themes": "^0.4.6",
    "react-day-picker": "^9.13.0",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0"
  }
}